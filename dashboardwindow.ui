      <property name="styleSheet">
       <string notr="true">/* Base style for QCheckBox */
QCheckBox {
  spacing: 8px; /* Space between the checkbox and the label */
  font-size: 16px;
  color: #333;
}

QCheckBox::indicator {
  width: 20px;
  height: 20px;
  border: 2px solid #d0e8ff;
  border-radius: 4px;
  background-color: #ffffff;
  transition: background-color 0.3s, border-color 0.3s;
}

QCheckBox::indicator:hover {
  border-color: #9ec8ff;
}

QCheckBox::indicator:checked {
  border-color: #0078d7;
  background-color: #0078d7; /* Blue box when checked */
