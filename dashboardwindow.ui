      <property name="styleSheet">
       <string notr="true">/* General styling for QTabWidget */
#tabWidget {
    background-color: transparent; /* Transparent background */
    border: none; /* Removes outer border */
    font-family: &quot;Segoe UI&quot;, sans-serif; /* Modern font */
    font-size: 14px; /* Standard font size */
    color: #333; /* Default text color */
}

/* Styling for the pane (content area) */
#tabWidget ::pane {
    border: 1px solid #87CEEB; /* Light blue border */
    border-radius: 8px; /* Rounded corners */
    padding: 10px; /* Inner padding for the content area */
    background: #f0f8ff; /* Light AliceBlue background */
}

/* Styling for the tabs in the tab bar */
QTabBar::tab {
    background: #add8e6; /* LightBlue background for tabs */
    border: 1px solid #87CEEB; /* Light blue border */
    padding: 8px 16px; /* Padding for the tabs */
    border-top-left-radius: 6px; /* Rounded top-left corners */
    border-top-right-radius: 6px; /* Rounded top-right corners */
    margin-right: 2px; /* Space between tabs */
    color: #333; /* Darker text for better contrast */
}

/* Styling for the selected tab */
QTabBar::tab:selected {
    background: #ffffff; /* White background for the selected tab */
    border-color: #4682B4; /* SteelBlue border for the selected tab */
    color: #000; /* Black text for the selected tab */
}

/* Styling for the hovered tab */
QTabBar::tab:hover {
    background: #87CEFA; /* LightSkyBlue background for hovered tab */
    color: #111; /* Slightly darker text color when hovered */
}

/* Styling for unselected tabs */
QTabBar::tab:!selected {
    margin-top: 2px; /* Creates a small gap for unselected tabs */
}</string>
