    color: none;
}

QTabWidget::pane {
    border: 1px solid #87CEEB; /* Light blue border */
    border-radius: 8px;
    padding: 10px;
    background: #f0f8ff; /* AliceBlue background */
}

QTabBar::tab {
    background: #add8e6; /* LightBlue background for tabs */
    border: 1px solid #87CEEB; /* Light blue border */
    padding: 8px 16px;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    margin-right: 2px;
    color: #333; /* Darker text for contrast */
}

QTabWidget::pane {
    background: transparent; /* Makes the tab content area transparent */
    border: none; /* Removes the border around the content area */
}

QTabBar::tab:selected {
    background: #ffffff; /* White background for selected tab */
    border-color: #4682B4; /* SteelBlue border for selected tab */
    color: #000; /* Black text for selected tab */
}

QTabBar::tab:hover {
    background: #87CEFA; /* LightSkyBlue background when hovered */
    color: #111; /* Slightly darker text color when hovered */
}

QTabBar::tab:!selected {
    margin-top: 2px;
}</string>
